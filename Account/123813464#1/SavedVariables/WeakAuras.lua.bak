				["init"] = {
					["custom"] = "local _debug_ = false\n\nlocal runeIndex = 1\nlocal runeTotal = 0\nlocal mainWindowisOpen = false\nlocal reg = aura_env.region\n\nif not runeButton then\n    if _debug_ then print(\"Initing\") end\n    \n    runeColor = {}\n    runeSlot = {}\n    runeButton = {}\n    \n    colorTextureID = {\"1323035\", \"1323037\", \"1323038\", \"1323039\"}\n    colorName = {\"{rt6}X\",\"{rt3}K\",\"{rt1}H\",\"{rt2}N\"}\n    \n    -----------------------------------------------------------------------\n    \n    -- init Rune Slots --\n    for i=1,4 ,1 do\n        if not runeSlot[i] then\n            runeSlot[i] = CreateFrame(\"BUTTON\", nil, reg )\n        end\n        \n        runeSlot[i]:SetSize(48,48)\n        runeSlot[i]:SetPoint(\"BOTTOM\", reg,(i%2)*(-24)+(i%2-1)*(-24),(math.floor(i/2.2)-1)*(-48)+22)\n        runeSlot[i]:SetNormalTexture(\"134400\")\n        runeSlot[i]:SetAlpha(0.8)\n        \n        runeSlot[i].text = runeSlot[i]:CreateFontString(nil, \"ARTWORK\", \"GameFontNormal\")\n        runeSlot[i].text:SetFont(\"Fonts\\\\ARIALN.ttf\", 32, \"OUTLINE\")\n        runeSlot[i].text:SetPoint(\"CENTER\",0,0)\n        runeSlot[i].text:SetText(i)\n        \n        runeSlot[i]:Hide()\n        \n    end\n    \n    ---------------------------------\n    \n    -- init Rune buttons --\n    \n    ---------------------------------4 rune buttons\n    for i=1,4 ,1 do\n        if not runeButton[i] then\n            runeButton[i] = CreateFrame(\"Button\", nil, reg)\n        end\n        \n        runeButton[i]:SetSize(48,48)\n        runeButton[i]:SetPoint(\"TOP\", reg,(i-2.5)*48,-22)\n        runeButton[i]:SetNormalTexture(colorTextureID[i])\n        runeButton[i]:RegisterForClicks(\"AnyUp\")\n        runeButton[i]:SetScript(\"OnClick\", function (self, button, down)\n                if _debug_ then print(runeIndex..\". \"..colorName[i]) end\n                runeColor[runeIndex] = colorName[i]\n                runeSlot[runeIndex]:SetNormalTexture(colorTextureID[i])\n                runeButton[i]:Hide()\n                \n                runeTotal = runeTotal + i\n                runeIndex = runeIndex+1\n                if runeIndex == 4 then\n                    runeButton[1+2+3+4-runeTotal]:Click()\n                    \n                    btnSendtoChat:Click()\n                end\n        end)\n        runeButton[i]:Hide()\n        \n    end\n    \n    ---------------------------------OK button\n    if not btnSendtoChat then\n        btnSendtoChat = CreateFrame(\"Button\", nil, reg)\n    end\n    \n    btnSendtoChat:SetSize(48,48)\n    btnSendtoChat:SetPoint(\"TOP\", reg,-24,-70)\n    btnSendtoChat:SetNormalTexture(\"Interface\\\\AddOns\\\\WeakAuras\\\\Media\\\\Textures\\\\ok-icon.tga\")\n    btnSendtoChat:RegisterForClicks(\"AnyUp\")\n    \n    for i=1,4,1 do\n        if runeColor[i] == nil then \n            runeColor[i] = \"??\"\n        end\n    end\n    \n    btnSendtoChat:SetScript(\"OnClick\", function (self, button, down)\n            if _debug_ then \n                print(\"send\")\n                SendChatMessage(\"1. \"..runeColor[1]..\"    2. \"..runeColor[2], \"say\", nil)\n                SendChatMessage(\"3. \"..runeColor[3]..\"    4. \"..runeColor[4], \"say\", nil)\n            else\n                SendChatMessage(\"1. \"..runeColor[1]..\"    2. \"..runeColor[2], \"PARTY\", nil)\n                SendChatMessage(\"3. \"..runeColor[3]..\"    4. \"..runeColor[4], \"PARTY\", nil)\n            end\n            btnCancelAll:Click()\n    end)\n    btnSendtoChat:Hide()\n    \n    ---------------------------------Cancel button\n    if not btnCancelAll then\n        btnCancelAll = CreateFrame(\"Button\", nil, reg)\n    end\n    \n    btnCancelAll:SetSize(48,48)\n    btnCancelAll:SetPoint(\"TOP\", reg,24,-70)\n    btnCancelAll:SetNormalTexture(\"Interface\\\\AddOns\\\\WeakAuras\\\\Media\\\\Textures\\\\cancel-mark.tga\")\n    btnCancelAll:RegisterForClicks(\"AnyUp\")\n    btnCancelAll:SetScript(\"OnClick\", function (self, button, down)\n            if _debug_ then print(\"cancel\") end\n            \n            btnSendtoChat:Hide()\n            btnCancelAll:Hide()\n            for i=1,4,1 do\n                runeSlot[i]:SetNormalTexture(\"134400\")\n                runeButton[i]:Hide()\n                runeSlot[i]:Hide()\n                runeColor[i] = \"??\"\n            end\n            runeIndex = 1\n            runeTotal = 0\n            mainWindowisOpen = false\n            \n            reg._btnStart:Show()\n    end)\n    btnCancelAll:Hide()\n    \n    -- init ends --\n    \n    -----------------------------------------------------------------------\n    -- Start button --\n    if not reg._btnStart then\n        reg._btnStart = CreateFrame(\"BUTTON\", nil, reg)\n    end\n    reg._btnStart.text = reg._btnStart:CreateFontString(nil, \"ARTWORK\", \"GameFontNormal\")\n    reg._btnStart.text:SetFont(\"Fonts\\\\ARIALN.ttf\", 20, \"OUTLINE\")\n    reg._btnStart.text:SetPoint(\"CENTER\",0,0)\n    reg._btnStart.text:SetText(\"click me\")\n    reg._btnStart:SetAllPoints()\n    reg._btnStart:SetScript(\"OnClick\", function()\n            if _debug_ then print(\"Starting\") end\n            if mainWindowisOpen then return end\n            mainWindowisOpen = true\n            runeIndex = 1\n            runeTotal = 0\n            \n            -- Show buttons \n            for i=1,4 ,1 do\n                runeButton[i]:Show()\n                runeSlot[i]:Show()\n            end\n            \n            btnSendtoChat:Show()\n            btnCancelAll:Show()\n            \n            reg._btnStart:Hide()\n        end\n        \n    )\nend\n\nreturn true\n\n\n\n",

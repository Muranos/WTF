					["custom"] = "-- Honorax@Illidan & Natty :)\n\naura_env.IsShadowOiled = function(...)\n    for i = 1, select(\"#\", ...) do\n        local region = select(i, ...)\n        if region and region:GetObjectType() == \"FontString\" then\n            local text = region:GetText()\n            if text and (string.match(text, \"Масло\")) then\n                local _,_, mins = string.find(text, \"%(([1-4]) мин\")\n                local _,_, seconds = string.find(text, \"%((%d+) сек\")\n                if mins then\n                    aura_env.remainingTime = mins * 60\n                    return false\n                elseif seconds then\n                    aura_env.remainingTime = seconds\n                    return false\n                end\n                return true\n            end\n            \n        end\n    end\n    aura_env.remainingTime = 0\n    return false\nend\n\naura_env.remainingTime = 0\naura_env.checkTime = 0\nif not (aura_env.weaponShadowFrame) then\n    aura_env.weaponShadowFrame = CreateFrame(\"GameTooltip\", \"WeaponShadowTooltip\", nil, \"GameTooltipTemplate\")\nend\naura_env.weaponShadowFrame:SetOwner(WorldFrame, \"ANCHOR_NONE\")\naura_env.weaponShadowFrame:ClearLines()\naura_env.weaponShadowFrame:SetInventoryItem(\"player\", 16)\naura_env.weaponShadowFrame:AddFontStrings(\n    aura_env.weaponShadowFrame:CreateFontString(\"$parentTextLeft1\", nil, \"GameTooltipText\"),\n    aura_env.weaponShadowFrame:CreateFontString(\"$parentTextRight1\", nil, \"GameTooltipText\"))\n\nif not aura_env.clickableFrame then\n    local r = WeakAuras.regions['ShadowcoreOil'].region\n    aura_env.clickableFrame = CreateFrame(\"Button\",\"ShadowcoreOilButton\", r, \"SecureActionButtonTemplate\")  \nend\n\naura_env.clickableFrame:SetAllPoints()\naura_env.clickableFrame:SetAttribute(\"type\", \"macro\")\naura_env.clickableFrame:SetAttribute(\"macrotext\", \"/use Масло теневого ядра\\n/use 16\\n/click StaticPopup1Button1\")",

n[i]:SetPoint(\"TOP\", aura_env.region,(i-2.5)*48,-22)\n            aura_env.runeButton[i]:SetNormalTexture(aura_env.colorTextureID[i])\n            aura_env.runeButton[i]:RegisterForClicks(\"AnyUp\")\n            aura_env.runeButton[i]:SetScript(\"OnClick\", function (self, button, down)\n                    if aura_env.debug then print(aura_env.colorName[i]) end\n                    aura_env.runeColor[aura_env.runeIndex] = aura_env.colorName[i]\n                    \n                    aura_env.runeSlot[aura_env.runeIndex].tex:SetTexture(aura_env.colorTextureID[i])\n                    aura_env.runeButton[i]:Hide()\n                    \n                    aura_env.runeIndex = aura_env.runeIndex+1\n            end)\n            aura_env.runeButton[i]:Show()\n        end\n        \n        ---------------------------------OK button\n        if not aura_env.btnSendtoChat then\n            aura_env.btnSendtoChat = CreateFrame(\"Button\", nil, aura_env.region, \"SecureActionButtonTemplate\")\n        end\n        \n        aura_env.btnSendtoChat:SetSize(48,48)\n        aura_env.btnSendtoChat:SetPoint(\"TOP\", aura_env.region,-24,-70)\n        aura_env.btnSendtoChat:SetNormalTexture(\"Interface\\\\AddOns\\\\WeakAuras\\\\Media\\\\Textures\\\\ok-icon.tga\")\n        aura_env.btnSendtoChat:RegisterForClicks(\"AnyUp\")\n        \n        for i=1,4,1 do\n            if aura_env.runeColor[i] == nil then \n                aura_env.runeColor[i] = \"??\"\n            end\n        end\n        \n        aura_env.btnSendtoChat:SetScript(\"OnClick\", function (self, button, down)\n                print(\"send\")\n                SendChatMessage(\"1. \"..\"{\"..aura_env.runeColor[1]..\"}\"..\"    2. \"..\"{\"..aura_env.runeColor[2]..\"}\", \"WHISPER\", nil, \"Sunnyrain\")\n                SendChatMessage(\"3. \"..\"{\"..aura_env.runeColor[3]..\"}\"..\"    4. \"..\"{\"..aura_env.runeColor[4]..\"}\", \"WHISPER\", nil, \"Sunnyrain\")\n                \n                aura_env.btnCancelAll:Click()\n        end)\n        \n        ---------------------------------Cancel button\n        if not aura_env.btnCancelAll then\n            aura_env.btnCancelAll = CreateFrame(\"Button\", nil, aura_env.region, \"SecureActionButtonTemplate\")\n        end\n        \n        aura_env.btnCancelAll:SetSize(48,48)\n        aura_env.btnCancelAll:SetPoint(\"TOP\", aura_env.region,24,-70)\n        aura_env.btnCancelAll:SetNormalTexture(\"Interface\\\\AddOns\\\\WeakAuras\\\\Media\\\\Textures\\\\cancel-mark.tga\")\n        aura_env.btnCancelAll:RegisterForClicks(\"AnyUp\")\n        aura_env.btnCancelAll:SetScript(\"OnClick\", function (self, button, down)\n                if aura_env.debug then print(\"cancel\") end\n                \n                aura_env.btnSendtoChat:Hide()\n                aura_env.btnCancelAll:Hide()\n                for i=1,4,1 do\n                    aura_env.runeButton[i]:Hide()\n                    aura_env.runeSlot[i]:Hide()\n                    \n                    aura_env.runeColor[i] = nil\n                    aura_env.runeButton[i] = nil\n                    aura_env.runeSlot[i] = nil\n                end\n                aura_env.runeIndex = 1\n                \n                aura_env.region._btnStart:Show()\n        end)\n        \n        -- init ends --\n        \n        aura_env.btnSendtoChat:Show()\n        aura_env.btnCancelAll:Show()\n        \n        for i=1,4,1 do\n            aura_env.runeButton[i]:Show()\n            aura_env.runeSlot[i]:Show()\n        end\n        \n        aura_env.region._btnStart:Hide()\n    end\n    \n)\n\nreturn true",

		["ZenTracker (ZT) Main"] = {
			["outline"] = "OUTLINE",
			["xOffset"] = -699.0000305175781,
			["displayText"] = " ",
			["yOffset"] = -25.999755859375,
			["anchorPoint"] = "CENTER",

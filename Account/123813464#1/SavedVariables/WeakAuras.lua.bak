			["customText"] = "function()\n    local c = aura_env.config\n    if aura_env.state and aura_env.state.current and aura_env.state.total then\n        -----------------------------------------------------\n        local text3, pride = \"\", false\n        if c.cPull and aura_env.MDT then\n            if aura_env.state.pullText or aura_env.state.mcpullText then\n                if c.pullMC and aura_env.state.mcpullCompare then\n                    text3 = aura_env.state.mcpullText\n                    pride = aura_env.state.mcpullCompare <= 0\n                elseif aura_env.state.pullCompare then\n                    text3 = aura_env.state.pullText\n                    pride = aura_env.state.pullCompare <= 0\n                end\n                \n                if pride then\n                    local r, g, b = c.pridefulColor[1], c.pridefulColor[2], c.pridefulColor[3]\n                    local color = CreateColor(r, g, b, 1)\n                    text3 = color:WrapTextInColorCode(text3)\n                else\n                    local r, g, b = c.textColor[1], c.textColor[2], c.textColor[3]\n                    local color = CreateColor(r, g, b, 1)\n                    text3 = color:WrapTextInColorCode(text3)\n                end\n            end\n        end\n        -----------------------------------------------------\n        local text1 = \"\"\n        if c.mobCountP then\n            local percent = string.format(\"%.1f\", aura_env.state.totalMC/100*aura_env.state.total)\n            text1 = (\"%s |cFFFFFFFF/|r |cFFe6cc80%s|r [%s]\"):format(aura_env.state.currentMC, percent, aura_env.state.totalMC)\n        else\n            text1 = (\"%s |cFFFFFFFF/|r |cFFe6cc80%s%%|r\"):format(aura_env.state.current, aura_env.state.total)\n        end\n        \n        local text2 = c.mobCount and string.format(\"%.1f\", aura_env.state.mcCompare) or aura_env.state.left\n        return text1, text2, text3\n    end\nend",

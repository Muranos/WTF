f bar then\n                aura_env.ReadBar(bar, \"BW\")\n            end\n        end\n    elseif event == \"DBM_TimerStart\" then\n        local id = ...\n        if id then\n            local bar = WeakAuras.GetDBMTimerById(id)\n            if bar then\n                aura_env.ReadBar(bar, \"DBM\")\n            end\n        end\n    elseif event == \"WAIT_CHAINS\" then\n        local setCounter = select(1, ...)\n        if aura_env.assignedSet and aura_env.assignedSet == tonumber(setCounter) then\n            states[setCounter] = {\n                changed = true,\n                show = true,\n                marker = aura_env.assignedMarker,\n                progressType = \"timed\",\n                duration = aura_env.config.alertDuration,\n                autoHide = true\n            }\n            return true\n        end\n    end\nend",

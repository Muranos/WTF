			["customText"] = "function()\n    if WeakAuras.IsOptionsOpen() then\n        return ICON_LIST[1] .. \"0|t\", WA_ClassColorName(\"player\"), WA_ClassColorName(\"player\")\n    end\n    \n    if aura_env.state then\n        local mark = aura_env.state.index <= 6 and ICON_LIST[aura_env.state.index] .. \"0|t\" or \"|T\" .. 4067925 .. \":0|t\"\n        local name = aura_env.state.dead and string.format(\"|cffff0000%s|r\", UnitName(aura_env.state.unit)) or WA_ClassColorName(aura_env.state.unit)\n        local backup = aura_env.state.backup and WA_ClassColorName(aura_env.state.backup) or \"\"\n        \n        return mark, name, backup\n    end\nend",

		["[NOT LOADED] Multi-Dungeon Mechanics (Seasonal Affixes for example) Season 1"] = {
			["iconSource"] = -1,
			["wagoID"] = "B-saNwgCJ",
			["parent"] = "Spell CDs on Nameplate - Dragonflight Season 1",
			["preferToUpdate"] = false,
			["customText"] = "",
			["yOffset"] = 0,
			["anchorPoint"] = "CENTER",
			["cooldownSwipe"] = true,

				["start"] = {
				},
				["init"] = {
					["custom"] = "aura_env.backupClasses = {\n    [1] = \"MAGE\",\n    [2] = \"HUNTER\",\n    [3] = \"ROGUE\",\n    [4] = \"DEMONHUNTER\",\n    [5] = \"PALADIN\",\n    [6] = \"DRUID\",\n    [7] = \"WARRIOR\",\n    [8] = \"MONK\",\n    [9] = \"SHAMAN\",\n    [10] = \"WARLOCK\",\n    [11] = \"DEATHKNIGHT\",\n    [12] = \"PRIEST\"\n}",
					["do_custom"] = true,

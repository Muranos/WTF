 \"table\" then\n    if aura_env.config.debug then print(\"Registering BigWigs\") end\n    WeakAuras.RegisterBigWigsTimer()\nelseif type(DBM) == \"table\" then\n    if aura_env.config.debug then print(\"Registering DBM\") end\n    WeakAuras.RegisterDBMCallback(\"DBM_TimerStart\")\nend\n\naura_env.ReadBar = function(bar, type)\n    if bar and not bar.cast and bar.expirationTime then\n        local start = (bar.expirationTime - GetTime()) - aura_env.config.alertDuration\n        if aura_env.config.debug then\n            local text = type == \"BW\" and bar.text or bar.message\n            print(\"Reading Bar: [\", bar.spellId, \"]\", text)\n        end\n        \n        if bar.spellId and tonumber(bar.spellId) == 350411 and bar.count then\n            C_Timer.After(start, function() WeakAuras.ScanEvents(\"WAIT_CHAINS\", tonumber(bar.count)) end)\n        end\n    end\nend",

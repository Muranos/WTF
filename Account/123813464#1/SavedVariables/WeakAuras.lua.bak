						["subeventPrefix"] = "SPELL",
						["custom"] = "function(states, event, ...)\n    if event == \"OPTIONS\" then\n        local progress = 25\n        local total = 40\n        local currentMC = 5\n        local totalMC =  460\n        local mobCount = 5/460\n        states[\"\"] = {\n            name = \"Prideful Tracker\",\n            progressType = \"static\",\n            value = progress,\n            total = total,\n            currentMC = currentMC,\n            totalMC = totalMC,\n            mobCount = mobCount,\n            sound = false,\n            pull = {},\n            pullText = \"5%\",\n            mcpullText = \"27\",\n            pullCompare = 1,\n            mcpullCompare = 1,\n            mcCompare = totalMC/100*total - currentMC,\n            leftCompare = total - progress,\n            current = string.format(\"%.2f%%\", progress),\n            left = string.format(\"%.2f%%\", total - progress),\n            glow = false,\n            show = true,\n            changed = true,\n        }\n        return true\n    end\n    \n    local state = states[\"\"]\n    local update = false\n    local c = aura_env.config\n    if event == \"SCENARIO_CRITERIA_UPDATE\" or event == \"WORLD_STATE_TIMER_START\" then\n        local progress, mobCount, currentMC, totalMC = aura_env.GetProgress()\n        \n        if progress then\n            local glow, sound = false, false\n            \n            if (not aura_env.MDT or not c.cPull) and c.glow then\n                glow = (progress >= 15 and progress < 20)\n                or (progress >= 35 and progress < 40)\n                or (progress >= 55 and progress < 60)\n                or (progress >= 75 and progress < 80)\n                or (progress >= 95 and progress < 100) or false\n            end\n            \n            local total = (progress < 20 and 20)\n            or (progress < 40 and 40)\n            or (progress < 60 and 60)\n            or (progress < 80 and 80)\n            or 100\n            \n            if aura_env.total and aura_env.total < total then\n                aura_env.total = total\n                sound = c.sound\n                if c.chat then\n                    SendChatMessage(c.message or \"\", \"PARTY\")\n                end\n            end\n            \n            if not aura_env.finish and progress == 100 then\n                aura_env.finish = true\n                sound = c.sound\n                if c.chat then\n                    SendChatMessage(c.message or \"\", \"PARTY\")\n                end\n            end\n            \n            if not state then\n                states[\"\"] = {\n                    name = \"Prideful Tracker\",\n                    progressType = \"static\",\n                    value = progress,\n                    total = total,\n                    currentMC = currentMC,\n                    totalMC = totalMC,\n                    mobCount = mobCount,\n                    sound = false,\n                    pull = {},\n                    pullText = \"\",\n                    mcpullText = \"\",\n                    pullCompare = 1,\n                    mcpullCompare = 1,\n                    mcCompare = totalMC/100*total - currentMC,\n                    leftCompare = total - progress,\n                    current = string.format(\"%.2f%%\", progress),\n                    left = string.format(\"%.2f%%\", total - progress),\n                    glow = glow,\n                    show = true,\n                    changed = true,\n                }\n                aura_env.total = total\n                aura_env.finish = progress == 100\n                update = true\n            elseif progress <= 100 then\n                state.sound = sound\n                state.value = progress\n                state.total = total\n                state.mobCount = mobCount\n                state.currentMC = currentMC\n                state.totalMC = totalMC\n                state.mcCompare = totalMC/100*total - currentMC\n                state.leftCompare = total - progress\n                state.current = string.format(\"%.2f%%\", progress)\n                state.left = string.format(\"%.2f%%\", total - progress)\n                if (not aura_env.MDT or not c.cPull) and c.glow then\n                    state.glow = glow\n                end\n                state.changed = true\n                update = true\n            end\n        end\n    end\n    \n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" then\n        local _, message, _, _, _, _, _, destGUID = ...\n        if message == \"UNIT_DIED\" then\n            if state and not aura_env.finish then\n                if aura_env.MDT and destGUID and state.pull[destGUID] then\n                    state.pull[destGUID] = \"DEAD\"\n                    update = true\n                end\n            end\n        end\n    end\n    \n    if c.cPull and aura_env.MDT and state then\n        if event == \"UNIT_THREAT_LIST_UPDATE\" and InCombatLockdown() then\n            local unit = ...\n            if unit and UnitExists(unit) then\n                local guid = UnitGUID(unit)\n                if guid and not state.pull[guid] then\n                    local npc_id = select(6, strsplit(\"-\", guid))\n                    if npc_id then\n                        local value, max= aura_env.MDT:GetEnemyForces(tonumber(npc_id))\n                        if value then\n                            state.pull[guid] = {value, value / max * 100}\n                            update = true\n                        end\n                    end\n                end            \n            end\n        end\n    end\n    \n    if (event == \"ENCOUNTER_END\" or event == \"PLAYER_REGEN_ENABLED\" or event == \"PLAYER_DEAD\") and state then\n        --do i need encounter_end?\n        for k, v in pairs(state.pull) do\n            state.pull[k] = nil\n        end\n        update = true\n    end\n    \n    if update and state then\n        local rawValue, percentValue = 0, 0\n        for guid, value in pairs(state.pull) do \n            if value ~= \"DEAD\" then\n                rawValue = rawValue + value[1]\n                percentValue = percentValue + value[2]\n            end\n        end\n        \n        local rawtext, text = \"\", \"\"\n        if percentValue > 0 or rawValue > 0 then\n            rawtext = rawValue\n            text = percentValue\n        end\n        \n        state.mcpullCompare = state.mcCompare - rawValue\n        state.mcpullText = rawtext\n        \n        state.pullCompare = state.leftCompare - percentValue\n        state.pullText = text ~= \"\" and string.format(\"%.2f%%\", text) or text\n        \n        local glow = false\n        if c.glow then\n            glow = state.pullCompare <= 0 or state.mcpullCompare <= 0\n        end\n        \n        if state.glow ~= glow then\n            state.glow = glow\n        end\n        state.changed = true\n        return true\n    end\nend\n\n\n",
